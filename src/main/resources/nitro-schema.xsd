                <xs:group ref="contributions_mixin" minOccurs="0"/>
                <xs:group ref="previous_next_mixin" minOccurs="0"/>
                <xs:group ref="availability_mixin" minOccurs="0"/>
                <xs:group ref="versions_mixin" minOccurs="0"/>
                <xs:element ref="genre_groupings" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="broadcast">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element name="service" type="serviceReference" minOccurs="0"/>
                <xs:element ref="updated_time" minOccurs="0"/>
                <xs:element ref="published_time" minOccurs="0"/>
                <xs:element ref="tx_time" minOccurs="0"/>
                <xs:element ref="ids" minOccurs="0"/>
                <xs:element ref="is_blanked" minOccurs="0"/>
                <xs:element ref="is_repeat" minOccurs="0"/>
                <xs:element ref="is_audio_described" minOccurs="0"/>
                <xs:element ref="is_critical" minOccurs="0"/>
                <xs:element ref="is_simulcast" minOccurs="0"/>
                <xs:element ref="broadcast_of" minOccurs="0" maxOccurs="unbounded"/>
                <xs:group ref="image_link" minOccurs="0"/>
                <xs:group ref="images_mixin" minOccurs="0"/>
                <xs:element ref="ancestors_titles" minOccurs="0"/>
                <xs:element ref="ancestor_titles" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="window">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element name="service" type="serviceReference" minOccurs="0"/>
                <xs:element ref="updated_time" minOccurs="0"/>
                <xs:element ref="scheduled_time" minOccurs="0"/>
                <xs:element ref="accurate_programme_time" minOccurs="0"/>
                <xs:element ref="media_availability_time" minOccurs="0"/>
                <xs:element ref="ids" minOccurs="0"/>
                <xs:element ref="window_of" minOccurs="0" maxOccurs="unbounded"/>
                <xs:group ref="image_link" minOccurs="0"/>
                <xs:element ref="scheduled_publication_rights" minOccurs="0"/>
                <xs:group ref="images_mixin" minOccurs="0"/>
                <xs:element ref="ancestors_titles" minOccurs="0"/>
                <xs:element ref="ancestor_titles" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="service">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="sid"/>
                <xs:element ref="ids" minOccurs="0"/>
                <xs:element ref="partner"/>
                <xs:element ref="updated_time" minOccurs="0"/>
                <xs:element ref="name" minOccurs="0"/>
                <xs:element ref="description" minOccurs="0"/>
                <xs:element ref="region" minOccurs="0"/>
                <xs:element ref="type" minOccurs="0"/>
                <xs:element ref="media_type" minOccurs="0"/>
                <xs:element ref="is_concrete" minOccurs="0"/>
                <xs:element ref="is_streamable" minOccurs="0"/>
                <xs:element ref="is_downloadable" minOccurs="0"/>
                <xs:element ref="date_range" minOccurs="0"/>
                <xs:group ref="master_brand_link" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="item">
        <xs:sequence>
            <xs:element ref="pid"/>
            <xs:element ref="partner"/>
            <xs:element ref="updated_time" minOccurs="0"/>
            <xs:element ref="ids" minOccurs="0"/>
            <xs:element name="type" minOccurs="0" type="xs:string"/>
            <xs:element ref="title" minOccurs="0"/>
            <xs:element ref="synopses" minOccurs="0"/>
            <xs:element ref="release_title" minOccurs="0"/>
            <xs:element ref="source_media" minOccurs="0"/>
            <xs:element ref="catalogue_number" minOccurs="0"/>
            <xs:element ref="record_label" minOccurs="0"/>
            <xs:element ref="publisher" minOccurs="0"/>
            <xs:element ref="music_code" minOccurs="0"/>
            <xs:element ref="track_number" minOccurs="0"/>
            <xs:element ref="track_side" minOccurs="0"/>
            <xs:element ref="recording_date" minOccurs="0"/>
            <xs:element name="duration" minOccurs="0" type="xs:float"/>
            <xs:element ref="item_of" minOccurs="0"/>
            <xs:element ref="offsets" minOccurs="0"/>
            <xs:element ref="play_events" minOccurs="0"/>
            <xs:element ref="snippet_url" minOccurs="0"/>
            <xs:group ref="image_link" minOccurs="0" maxOccurs="unbounded"/>
            <xs:group ref="images_mixin" minOccurs="0"/>
            <xs:group ref="contributions_mixin" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="offsets">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="offset_in" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="play_events">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="play_event" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="offset_in">
        <xs:complexType>
            <xs:attribute name="programme" type="pid" use="required"/>
            <xs:attribute name="version" type="pid" use="required"/>
            <xs:attribute name="position" type="xs:decimal"/>
            <xs:attribute name="offset" type="xs:integer"/>
            <xs:attribute name="href" type="xs:anyURI" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="play_event">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="title" minOccurs="0" type="xs:string"/>
                <xs:element ref="synopses" minOccurs="0"/>
                <xs:group ref="played_in_links" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="pid" type="pid" use="required"/>
            <xs:attribute name="offset" type="xs:integer"/>
            <xs:attribute name="position" type="xs:decimal"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="played_in_links">
        <xs:sequence>
            <xs:element name="played_in" minOccurs="0">
                <xs:complexType>
                    <xs:attribute name="result_type" type="xs:string"/>
                    <xs:attribute name="href" type="xs:anyURI"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="availability_mixin">
        <xs:sequence>
            <xs:element name="availability" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element ref="status"/>
                        <xs:element name="version_types">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="version_type" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:attribute name="id" type="xs:string"/>
                                            <xs:attribute name="start" type="xs:dateTime"/>
                                            <xs:attribute name="end" type="xs:dateTime"/>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="versions_mixin">
        <xs:sequence>
            <xs:element ref="versions" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="available_versions" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:group>
    <xs:element name="versions">
        <xs:complexType>
            <xs:group ref="version_availability_mixin"/>
            <xs:attribute name="available" type="xs:integer" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="version_availability_mixin">
        <xs:sequence>
            <xs:element name="version" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="duration" minOccurs="0" maxOccurs="1" type="xs:duration"/>
                        <xs:element name="competition_warning" minOccurs="0" maxOccurs="1" type="xs:string"/>
                        <xs:group ref="guidance_warnings" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="types" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="type" minOccurs="0" maxOccurs="unbounded" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="availabilities" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:group ref="version_availability" maxOccurs="unbounded"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="pid" type="pid"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="version_availability">
        <xs:sequence>
            <xs:element name="availability">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="availability_start" minOccurs="0" maxOccurs="1" type="xs:dateTime"/>
                        <xs:element name="availability_end" minOccurs="0" maxOccurs="1" type="xs:dateTime"/>
                        <xs:group ref="available_media_sets" maxOccurs="1"/>
                    </xs:sequence>
                    <xs:attribute name="status" type="xs:string"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:element name="available_versions">
        <xs:complexType>
            <xs:group ref="available_versions_mixin"/>
            <xs:attribute name="available" type="xs:integer" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="available_versions_mixin">
        <xs:sequence>
            <xs:element name="version" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="duration" minOccurs="0" maxOccurs="1" type="xs:duration"/>
                        <xs:element name="competition_warning" minOccurs="0" maxOccurs="1" type="xs:boolean"/>
                        <xs:group ref="guidance_warnings" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="types" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="type" minOccurs="0" maxOccurs="unbounded" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="availabilities" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:group ref="available_versions_availability" maxOccurs="unbounded"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="pid" type="pid"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="available_versions_availability">
        <xs:sequence>
            <xs:element name="availability">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="scheduled_start" minOccurs="0" maxOccurs="1" type="xs:dateTime"/>
                        <xs:element name="scheduled_end" minOccurs="0" maxOccurs="1" type="xs:dateTime"/>
                        <xs:group ref="available_versions_media_sets" maxOccurs="1"/>
                    </xs:sequence>
                    <xs:attribute name="status" type="xs:string"/>
                    <xs:attribute name="type" type="xs:string"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="version_types_with_id">
        <xs:sequence>
            <xs:element name="version_types">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="version_type" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="id" type="xs:string"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="types_with_id">
        <xs:sequence>
            <xs:element name="types">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="type" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="id" type="xs:string"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="available_media_sets">
        <xs:sequence>
            <xs:element name="media_sets" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="media_set" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:simpleContent>
                                    <xs:extension base="xs:string"/>
                                </xs:simpleContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="available_versions_media_sets">
        <xs:sequence>
            <xs:element name="media_sets" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="media_set" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="actual_start" minOccurs="0" maxOccurs="1" type="xs:dateTime"/>
                                    <xs:element name="name" minOccurs="1" maxOccurs="1" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:element name="warning_texts">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="warning_text" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
