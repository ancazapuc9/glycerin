                <xs:element minOccurs="0" ref="sorts"/>
                <xs:element minOccurs="0" ref="mixins"/>
                <xs:element minOccurs="0" ref="deprecations"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="pagination">
        <xs:complexType>
