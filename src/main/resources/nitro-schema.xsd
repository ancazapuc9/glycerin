<?xml version="1.0" encoding="UTF-8"?><xs:schema targetNamespace="http://www.bbc.co.uk/nitro/" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns="http://www.bbc.co.uk/nitro/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="nitro">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="pagination"/>
