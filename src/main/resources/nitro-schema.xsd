                <xs:element ref="availability"/>
                <xs:element ref="window"/>
                <xs:element ref="franchise"/>
                <xs:element ref="season"/>
                <xs:element ref="collection"/>
                <xs:element ref="gallery"/>
                <xs:element ref="promotion"/>
                <xs:element ref="master_brand"/>
                <xs:element ref="image"/>
            </xs:choice>
            <xs:attribute name="more_than" type="xs:integer"/>
            <xs:attribute name="total" type="xs:integer"/>
            <xs:attribute name="page" use="required" type="xs:integer"/>
            <xs:attribute name="page_size" use="required" type="xs:integer"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="promotion">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element ref="updated_time"/>
                <xs:element ref="title" minOccurs="0"/>
