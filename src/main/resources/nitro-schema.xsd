                <xs:element ref="promoted_for" minOccurs="0"/>
                <xs:element ref="context" minOccurs="0"/>
                <xs:element ref="status" minOccurs="0"/>
                <xs:element ref="url" minOccurs="0"/>
                <xs:element ref="promotion_of" minOccurs="0"/>
                <xs:element ref="related_links" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="brand">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element ref="updated_time"/>
                <xs:element ref="embargoed"/>
                <xs:element ref="title" minOccurs="0"/>
                <xs:element ref="synopses" minOccurs="0"/>
                <xs:element ref="uri" minOccurs="0"/>
                <xs:group ref="image_link" minOccurs="0"/>
                <xs:group ref="release_date_group" minOccurs="0"/>
                <xs:group ref="master_brand_link" minOccurs="0"/>
                <xs:element ref="franchises" minOccurs="0"/>
                <xs:element ref="items_for" minOccurs="0"/>
                <xs:element ref="expected_child_count" minOccurs="0"/>
                <xs:element ref="programme_formats" minOccurs="0"/>
                <xs:group ref="images_mixin" minOccurs="0"/>
                <xs:group ref="alternate_images_mixin" minOccurs="0"/>
                <xs:element ref="is_embeddable" minOccurs="0"/>
                <xs:element ref="related_links" minOccurs="0"/>
                <xs:group ref="people_mixin" minOccurs="0"/>
                <xs:group ref="contributions_mixin" minOccurs="0"/>
                <xs:group ref="previous_next_mixin" minOccurs="0"/>
                <xs:group ref="availability_mixin" minOccurs="0"/>
                <xs:element ref="genre_groupings" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="series">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element ref="updated_time"/>
                <xs:element ref="embargoed"/>
                <xs:element ref="title" minOccurs="0"/>
                <xs:element ref="synopses" minOccurs="0"/>
                <xs:element ref="uri" minOccurs="0"/>
                <xs:group ref="image_link" minOccurs="0"/>
                <xs:group ref="release_date_group" minOccurs="0"/>
                <xs:group ref="master_brand_link" minOccurs="0"/>
                <xs:element ref="series_of" minOccurs="0"/>
                <xs:element ref="ancestors" minOccurs="0"/>
                <xs:element ref="items_for" minOccurs="0"/>
                <xs:element ref="expected_child_count" minOccurs="0"/>
                <xs:element ref="programme_formats" minOccurs="0"/>
                <xs:group ref="images_mixin" minOccurs="0"/>
                <xs:group ref="alternate_images_mixin" minOccurs="0"/>
                <xs:element ref="is_embeddable" minOccurs="0"/>
                <xs:element ref="ancestors_titles" minOccurs="0"/>
                <xs:element ref="ancestor_titles" minOccurs="0"/>
                <xs:element ref="related_links" minOccurs="0"/>
                <xs:group ref="people_mixin" minOccurs="0"/>
                <xs:group ref="contributions_mixin" minOccurs="0"/>
                <xs:group ref="previous_next_mixin" minOccurs="0"/>
                <xs:group ref="availability_mixin" minOccurs="0"/>
                <xs:element ref="genre_groupings" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="episode">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element ref="updated_time"/>
                <xs:element ref="embargoed"/>
                <xs:element ref="title" minOccurs="0"/>
                <xs:element ref="synopses" minOccurs="0"/>
                <xs:element ref="uri" minOccurs="0"/>
                <xs:element ref="advertising_allowed"/>
                <xs:element ref="presentation_title" minOccurs="0"/>
                <xs:element ref="media_type" minOccurs="0"/>
                <xs:group ref="image_link" minOccurs="0"/>
                <xs:group ref="release_date_group" minOccurs="0"/>
                <xs:element ref="is_stacked" minOccurs="0"/>
                <xs:element ref="has_guidance" minOccurs="0"/>
                <xs:group ref="master_brand_link" minOccurs="0"/>
                <xs:element ref="episode_of" minOccurs="0"/>
                <xs:element ref="genre_groups" minOccurs="0"/>
                <xs:element ref="formats" minOccurs="0"/>
                <xs:element ref="ancestors" minOccurs="0"/>
                <xs:element ref="items_for" minOccurs="0"/>
                <xs:element ref="programme_formats" minOccurs="0"/>
                <xs:group ref="images_mixin" minOccurs="0"/>
                <xs:group ref="alternate_images_mixin" minOccurs="0"/>
                <xs:element ref="is_embeddable" minOccurs="0"/>
                <xs:element ref="ancestors_titles" minOccurs="0"/>
                <xs:element ref="ancestor_titles" minOccurs="0"/>
                <xs:group ref="original_version_duration" minOccurs="0"/>
                <xs:element ref="related_links" minOccurs="0"/>
                <xs:group ref="people_mixin" minOccurs="0"/>
                <xs:group ref="contributions_mixin" minOccurs="0"/>
                <xs:group ref="previous_next_mixin" minOccurs="0"/>
                <xs:group ref="availability_mixin" minOccurs="0"/>
                <xs:group ref="versions_mixin" minOccurs="0"/>
                <xs:element ref="genre_groupings" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="clip">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="pid"/>
                <xs:element ref="partner"/>
                <xs:element ref="updated_time"/>
                <xs:element ref="embargoed"/>
                <xs:element ref="title" minOccurs="0"/>
                <xs:element ref="synopses" minOccurs="0"/>
                <xs:element ref="uri" minOccurs="0"/>
                <xs:element ref="advertising_allowed"/>
                <xs:element ref="media_type" minOccurs="0"/>
                <xs:group ref="image_link" minOccurs="0"/>
                <xs:group ref="release_date_group" minOccurs="0"/>
                <xs:group ref="master_brand_link" minOccurs="0"/>
                <xs:element ref="clip_of" minOccurs="0"/>
                <xs:element ref="genre_groups" minOccurs="0"/>
                <xs:element ref="formats" minOccurs="0"/>
                <xs:element ref="ancestors" minOccurs="0"/>
                <xs:element ref="items_for" minOccurs="0"/>
                <xs:element ref="programme_formats" minOccurs="0"/>
                <xs:group ref="images_mixin" minOccurs="0"/>
                <xs:group ref="alternate_images_mixin" minOccurs="0"/>
                <xs:element ref="is_embeddable" minOccurs="0"/>
                <xs:element ref="ancestors_titles" minOccurs="0"/>
                <xs:element ref="ancestor_titles" minOccurs="0"/>
